<body>

    <script src="~/js/jquery-3.3.1.min.js" type="text/javascript"></script>

    @RenderBody()


    <script href="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.js"></script>
    <script src="~/hydro/js/jquery.js" type="text/javascript"></script>
    <script src="~/hydro/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/hydro/js/jquery.stellar.min.js" type="text/javascript"></script>
    <script src="~/hydro/js/jquery.magnific-popup.min.js" type="text/javascript"></script>
    <script src="~/hydro/js/smoothscroll.js" type="text/javascript"></script>
